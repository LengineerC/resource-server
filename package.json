{
  "name": "file-server",
  "version": "1.0.0",
  "main": "index.js",
  "bin": "dist/server/app.js",
  "pkg": {
    "scripts": [
      "dist/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-linux-x64"
    ],
    "outputPath": "release"
  },
  "scripts": {
    "compile": "tsc .",
    "test": "npx jest",
    "start:client": "cross-env NODE_ENV=development webpack serve --config webpack/client.config.dev.js",
    "start:server": "cross-env NODE_ENV=development webpack --config webpack/server.config.js && node dist/server/app.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/client.config.build.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server.config.js",
    "release": "cross-env NODE_ENV=production yarn build:client && cross-env NODE_ENV=production yarn build:server && pkg ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "commonjs",
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@jest/globals": "^29.7.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsftp": "^2.1.5",
    "@types/log4js": "^2.3.5",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "antd": "^5.24.6",
    "axios": "^1.8.4",
    "babel-loader": "^10.0.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "global": "^4.4.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jsftp": "^2.1.3",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "react-redux": "^9.2.0",
    "sass-loader": "^16.0.4",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "text-encoding": "^0.7.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "util": "^0.12.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "express": "^5.1.0",
    "log4js": "^6.9.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^7.5.0",
    "sass": "^1.83.4"
  }
}
